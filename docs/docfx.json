{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md",
          "SECURITY.md",
          "**/**/*-content.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/microsoft-cloud/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "zone_pivot_group_filename":
      {
      "tutorials/**/*.md": "tutorials/zone-pivot-groups.json"
      }
    },
    "fileMetadata": {},
    "template": [],
    "dest": "microsoft-cloud",
    "markdownEngineName": "markdig"
  }
}